class Animal{
	String name;
	Animal(String name){	
		this.name = name;
		System.out.println(name + " is an Animal");
	}
	void eats(){
		System.out.println("Animal eats . ");
	}
}
class Dog extends Animal{
	Dog(){
		super("ninetails");	
	}
	void barks(){
		System.out.println("Dog barks .");
	}
}
public class Superkey{
	public static void main(String[] args){
		Dog d = new Dog();
		d.eats();
		d.barks();
	}
}
